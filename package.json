{
  "name": "cypress-placeholder-replacer-command",
  "version": "1.0.0",
  "description": "A cypress command to replace placeholders within json object or string",
  "keywords": [
    "Cypress",
    "placeholder",
    "replacer"
  ],
  "author": "Benoit Wauthier",
  "license": "MIT",
  "main": "index.js",
